system sm.res

import dm.Reservations.*
import sm.ReservationsBase

core information model ReservationsCore { 

	grab aggregate dm.Reservations.Tables {
	
		grab root entity dm.Reservations.Table
		add {
			id : ID
		}
	
	}
	
	grab aggregate dm.Reservations.Parties {
	
	//	query all_Waiting_Parties() : Party (0..*) returns Party.all.select(p|p.state = Party.WAITING)
		grab root entity dm.Reservations.Party 
		add {
			id : ID
		}
	
	}
}