domain dm.Reservations 

primitive Size redefines Natural
primitive Message redefines Text

aggregate {
	
	enumeration TableState { FREE, USED }
	
	root entity Table {
		seats : Size
		state : TableState
		party : Party (0..1)
	}
	
	detail Party {
		name : Name
		size : Size
		arrived : Timepoint
		waiting : Boolean
	}
	
} 
			